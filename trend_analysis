#Read census data
data <- read.csv(file.choose(), sep=",", header=TRUE)
View(data)
attach(data)


# Plot trend Income distribution to No high school diploma
plot(PER.CAPITA.INCOME, PERCENT.AGED.25.WITHOUT.HIGH.SCHOOL.DIPLOMA, xlab="Per Capita Income", ylab="Percentage Aged 25 (No High School Diploma)")



